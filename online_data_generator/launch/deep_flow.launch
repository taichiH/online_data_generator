<launch>
  <arg name="input" default="/remote/openni_camera/rgb/image_rect_color" />

  <node name="deep_flow"
        pkg="nodelet" type="nodelet"
        args="standalone online_data_generator/DeepFlow"
        output="screen">
    <remap from="~input" to="$(arg input)" />
    <rosparam>
      debug: true
      threshold: 0.3
    </rosparam>
  </node>
</launch>
